{
  "name": "splintr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "sbom": "npx @cyclonedx/cyclonedx-npm --output bom.json",
    "licenses:report": "npx license-checker --production --json > licenses.json",
    "cap:init": "npx cap init Splintr co.splintr.app -y",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "assets:generate": "npx @capacitor/assets generate",
    "app:prepare:ios": "npm run build && npx cap copy ios",
    "app:prepare:android": "npm run build && npx cap copy android",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db reset --with-seed",
    "db:migrate": "supabase migration new",
    "db:push": "supabase db push",
    "db:types": "supabase gen types typescript --local > src/types/database.types.ts",
    "db:studio": "supabase studio",
    "admin:grant": "node scripts/admin/grant-admin.js",
    "admin:revoke": "node scripts/admin/revoke-admin.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.57.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.1",
    "@types/bull": "^3.15.9",
    "@types/formidable": "^3.4.5",
    "@types/ioredis": "^5.0.0",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1692.0",
    "bull": "^4.16.5",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "formidable": "^3.5.4",
    "ioredis": "^5.4.1",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "redis": "^5.8.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^12.0.0",
    "zod": "^4.1.5",
    "pino": "^9.3.2",
    "stripe": " ^16.6.0 ",
    "web-push": " ^3.6.6 ",
    "ytdl-core": " ^4.11.5 ",
    "@capacitor/core": " ^6.2.0 ",
    "@capacitor/push-notifications": " ^6.0.1 ",
    "@capacitor/camera": " ^6.0.1 ",
    "@capacitor/share": " ^6.0.1 ",
    "@capacitor/app": " ^6.0.1 ",
    "@capawesome/capacitor-background-task": " ^6.0.0 "
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@playwright/test": "^1.51.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "node-mocks-http": "^1.17.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "@capacitor/cli": " ^6.2.0 ",
    "pino-pretty": "^11.2.2",
    "license-checker": "^25.0.1",
    "@cyclonedx/cyclonedx-npm": "^1.18.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/resources": "^1.9.0",
    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.54.0",
    "@opentelemetry/instrumentation-fetch": "^0.54.0"
  }
}
